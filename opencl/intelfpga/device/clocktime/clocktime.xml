  <FUNCTION name="clocktime64" module="clocktime">
    <ATTRIBUTES>
      <IS_STALL_FREE value="yes" />
      <IS_FIXED_LATENCY value="yes" />
      <EXPECTED_LATENCY value="1" />
      <CAPACITY value="1" />
      <HAS_SIDE_EFFECTS value="no" />
      <ALLOW_MERGING value="no" />
      <PARAMETER name="IDATA_WIDTH" value="64" />
      <PARAMETER name="TIME_WIDTH" value="64" />
      <PARAMETER name="ODATA_PADS" value="0" />
    </ATTRIBUTES>
    <INTERFACE>
      <AVALON port="clock" type="clock" />
      <AVALON port="resetn" type="resetn" />
      <AVALON port="ivalid" type="ivalid" />
      <AVALON port="iready" type="iready" />
      <AVALON port="ovalid" type="ovalid" />
      <AVALON port="oready" type="oready" />
      <INPUT port="idata" width="64" />
      <OUTPUT port="odata" width="128" />
    </INTERFACE>
    <C_MODEL>
      <FILE name="clocktime_c.cl" />
    </C_MODEL>
    <REQUIREMENTS>
      <FILE name="clocktime.v" />
    </REQUIREMENTS>
  </FUNCTION>

